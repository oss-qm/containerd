#!/usr/bin/make -f

export DH_VERBOSE = 1
export GOVERSION=1.16
export GO=/usr/lib/go-$(GOVERSION)/bin/go
export PREFIX=/usr
export GO_BUILDTAGS=no_systemd

%:
	git config --global --add safe.directory `pwd`
	dh $@

override_dh_auto_test:
	: # do nothing

override_dh_auto_install:
	dh_auto_install -- install install-man
